{
    "name": "schoolspider/spider-editor",
    "description": "a package that parses json from the spider drag and drop builder",
    "keywords": [
        "schoolspider",
        "spider-editor"
    ],
    "homepage": "https://github.com/schoolspider/spider-editor",
    "license": "MIT",
    "version": "0.0.7",
    "authors": [
        {
            "name": "Simon Morris",
            "email": "simon@schoolspider.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.45"
    },
    "require-dev": {
        "pestphp/pest": "^2.15",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "SchoolSpider\\SpiderEditor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
